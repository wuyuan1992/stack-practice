{
  "name": "browser-tabs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "preview": "pnpm run build && wrangler pages dev ./dist",
    "deploy": "pnpm run build && wrangler pages deploy ./dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --tag-prefix v"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "antd": "^5.21.5",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.11.11",
    "immer": "^10.1.1",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.27.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "code-inspector-plugin": "^0.17.2",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "i18next": "^23.16.2",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react-i18next": "^15.0.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.0.0",
    "vite": "^5.4.8",
    "vite-plugin-replace-image-url": "^1.2.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.3",
    "wrangler": "^3.84.1"
  },
  "lint-staged": {
    "*.{css,ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix"
  }
}
